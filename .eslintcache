\r\n            console.log(error);\r\n        })\r\n\r\n    }\r\n\r\n    deleteBudget(id) {\r\n        axios.delete('https://final-project-node-server-pbfph.ondigitalocean.app/budget/'+id)\r\n        .then(response => { console.log(response.data)});\r\n\r\n        this.setState({\r\n            budgetItems: this.state.budgetItems.filter(el => el._id !== id)\r\n        })\r\n    }\r\n\r\n    filterBudgetList() {  \r\n        const name = this.props.username;\r\n        this.setState({\r\n            budgetItems: this.state.budgetItems.filter(currentbudget => currentbudget.username.includes(`${name}`) )\r\n        })\r\n    }\r\n\r\n    budgetList() {\r\n        return this.state.budgetItems.map(currentbudget => {\r\n        return <Budget budget={currentbudget} deleteBudget={this.deleteBudget} key={currentbudget._id}/>;\r\n        })\r\n    }\r\n\r\n}","C:\\Users\\surfe\\Desktop\\School\\Fall 2020\\ITIS 4166\\CMDER\\dev\\final-project\\src\\LoginPage\\SignUpPage.js",[],"C:\\Users\\surfe\\Desktop\\School\\Fall 2020\\ITIS 4166\\CMDER\\dev\\final-project\\src\\auth0-provider-with-history.js",["109"],"C:\\Users\\surfe\\Desktop\\School\\Fall 2020\\ITIS 4166\\CMDER\\dev\\final-project\\src\\components\\logout-button.js",[],"C:\\Users\\surfe\\Desktop\\School\\Fall 2020\\ITIS 4166\\CMDER\\dev\\final-project\\src\\components\\login-button.js",[],"C:\\Users\\surfe\\Desktop\\School\\Fall 2020\\ITIS 4166\\CMDER\\dev\\final-project\\src\\Menu\\Menu.js",[],"C:\\Users\\surfe\\Desktop\\School\\Fall 2020\\ITIS 4166\\CMDER\\dev\\final-project\\src\\Menu\\menu.js",[],"C:\\Users\\surfe\\Desktop\\School\\Fall 2020\\ITIS 4166\\CMDER\\dev\\final-project\\src\\components\\Loading.js",[],"C:\\Users\\surfe\\Desktop\\School\\Fall 2020\\ITIS 4166\\CMDER\\dev\\final-project\\src\\Dashboard\\profile.js",["110"],"import React from 'react';\r\nimport { Container, Row, Col } from 'react-bootstrap';\r\nimport { useAuth0 } from '@auth0/auth0-react';\r\n\r\nexport const Profile = () => {\r\n    \r\n  